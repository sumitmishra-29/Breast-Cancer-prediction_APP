{
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      },
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/app.py" },
      { "src": "/(.*)", "dest": "/src/index.js" }
    ]
  }
  
  